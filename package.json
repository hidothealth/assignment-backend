{
  "name": "hi.tech-stage",
  "version": "1.0.0",
  "description": "Microservice for technical interview",
  "main": "src/index.ts",
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts --watch src/",
    "build": "npm run compile",
    "test": "jest --watch",
    "test:ci": "jest --coverage",
    "lint": "tsc --noEmit && eslint '**/**/*.{js,ts,tsx}' --quiet --fix",
    "check": "gts check src/**/*.ts",
    "clean": "gts clean",
    "compile": "rm -rf build/ && tsc -p .",
    "fix": "gts fix",
    "start": "ts-node src/index.ts"
  },
  "author": "hi.health",
  "license": "ISC",
  "dependencies": {
    "@hidothealth/fastify": "^1.0.5",
    "aws-sdk": "2.804.0",
    "crypto-js": "4.0.0",
    "dd-trace": "0.33.0",
    "dotenv": "^16.0.1",
    "fastify": "^3.20.2",
    "fastify-swagger": "^4.7.0",
    "http-errors": "1.8.0",
    "stripe": "^8.219.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/http-errors": "1.8.0",
    "@types/jest": "26.0.23",
    "@types/node": "15.0.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "7.25.0",
    "faker": "5.5.3",
    "gts": "3.1.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-mock-extended": "1.0.14",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
